    <!--

    The color table can be found in the README.md.

    Sublime Text color scheme documentation:
    https://www.sublimetext.com/docs/3/color_schemes.html

    Here is the table of % opacity to hex values e.g. to specify red with 50%
    opacity use #ff000080: https://stackoverflow.com/a/28481374

      100% — FF
       95% — F2
       90% — E6
       85% — D9
       80% — CC
       75% — BF
       70% — B3
       65% — A6
       60% — 99
       55% — 8C
       50% — 80
       45% — 73
       40% — 66
       35% — 59
       30% — 4D
       25% — 40
       20% — 33
       15% — 26
       10% — 1A
        5% — 0D
        0% — 00

    Other Monokai themes:

    * Atom - https://github.com/kevinsawicki/monokai.

    -->
    <array>
        <dict>
            <key>settings</key>
            <dict>

                <!-- Global settings -->
                <key>background</key>                   <string>#272822</string>
                <key>foreground</key>                   <string>#f8f8f2</string>
                <key>caret</key>                        <string>#e6db74</string>
                <key>lineHighlight</key>                <string>#f8f8f21a</string> <!-- Alpha 1a ~= 10%. -->

                <!-- Accents -->
                <key>misspelling</key>                  <string>#f926729d</string>
                <key>minimapBorder</key>                <string>#e6db74</string>
                <key>accent</key>                       <string>#e6db749d</string>

                <!-- CSS -->
                <key>popupCss</key><string>
                  html {
                      background-color: var(--background);
                  }

                  body {
                      color: #cfcfc2;
                  }

                  a {
                    color: #66d9ef;
                  }

                  #show-definitions {
                      border: 1px solid #75715e;
                      background-color: color(var(--background) blend(white 98%));
                      padding: 10px;
                      margin: 0;
                  }

                  #inline-error {
                      font-family: "Roboto Mono", "Segoe UI Symbol", monospace;
                      background-color: var(--background);
                  }

                  #inline-error .error {
                      background-color: color(var(--redish) blend(var(--background) 30%));
                  }

                  .mdpopups {
                      border: 1px solid #75715e;
                  }
                </string>

                <!-- Gutter -->
                <key>gutter</key>                       <string>#272822</string>
                <key>gutterForeground</key>             <string>#75715e</string>

                <!-- Selection -->
                <key>selection</key>                    <string>#75715e</string>
                <key>selectionForeground</key>          <string>#272822</string>
                <key>selectionBorder</key>              <string></string>
                <key>inactiveSelection</key>            <string>#75715e</string>
                <!-- The inactive selection foreground doesn't work.
                See https://github.com/SublimeTextIssues/Core/issues/2118. -->
                <key>inactiveSelectionForeground</key>  <string>#272822</string>
                <!-- The inactive selection border doesn't work.
                See https://github.com/SublimeTextIssues/Core/issues/2118. -->
                <key>inactiveSelectionBorder</key>      <string></string>

                <!-- Find -->
                <key>highlight</key>                    <string>#e6db74</string>
                <key>findHighlight</key>                <string>#a1efe4</string>
                <key>findHighlightForeground</key>      <string>#272822</string>

                <!-- Guides -->
                <key>guide</key>                        <string>#75715e9d</string>
                <key>stackGuide</key>                   <string>#f8f8f280</string>
                <key>activeGuide</key>                  <string>#e6db7480</string>

                <!-- Brackets -->
                <!-- It's not possible to set the background for brackets.
                See https://github.com/SublimeTextIssues/Core/issues/2123. -->
                <key>bracketsOptions</key>              <string>underline</string>
                <key>bracketsForeground</key>           <string>#e6db74</string>
                <key>bracketContentsOptions</key>       <string>underline</string>
                <key>bracketContentsForeground</key>    <string>#e6db74</string>

                <!-- Tags -->
                <!-- It's not possible to set the background for tags.
                See https://github.com/SublimeTextIssues/Core/issues/2123. -->
                <key>tagsOptions</key>                  <string>stippled_underline</string>
                <key>tagsForeground</key>               <string>#e6db7480</string>

                <!-- Shadows -->
                <!-- <key>shadow</key>                       <string></string> -->
                <!-- <key>shadowWidth</key>                  <string></string> -->

                <!-- Beta -->
                <key>rulers</key>                       <string>#75715e9d</string>
                <key>invisibles</key>                   <string>#f8f8f226</string>

